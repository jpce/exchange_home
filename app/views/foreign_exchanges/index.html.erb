<p id="notice"><%= notice %></p>

<h1>Listing Foreign Exchanges</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foreign_exchanges.each do |foreign_exchange| %>
      <tr>
        <td><%= foreign_exchange.description %></td>
        <td><%= foreign_exchange.name %></td>
        <td><%= link_to 'Show', foreign_exchange %></td>
        <td><%= link_to 'Edit', edit_foreign_exchange_path(foreign_exchange) %></td>
        <td><%= link_to 'Destroy', foreign_exchange, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foreign exchange', new_foreign_exchange_path %>
